<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick />
        <OnSwipe motion:touchAnchorId="@+id/demo07_up"
            motion:dragDirection="dragUp"
            motion:touchAnchorSide="top" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/demo07_up"
            android:layout_width="130dp"
            android:layout_height="130dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toRightOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_09"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_08"
            android:layout_width="130dp"
            android:layout_height="130dp"
            motion:layout_constraintTop_toBottomOf="@id/demo07_icon_06"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/demo07_icon_08"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_07"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@id/demo07_icon_05" />
        <Constraint
            android:id="@+id/demo07_icon_07"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@id/demo07_icon_04"
            motion:layout_constraintLeft_toLeftOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_01"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_02"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_01"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_03"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_02"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_04"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_03" />
        <Constraint
            android:id="@+id/demo07_icon_05"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_04"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/demo07_icon_06"
            motion:layout_constraintLeft_toRightOf="@id/demo07_icon_05"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
</MotionScene>