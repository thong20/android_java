<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo_service">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <!--        android:name=".Demo03_Foreground_Notification.MyApplication"-->
    <application
        android:name=".Demo05_Foreground_Media_Control.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Demo_Service">
        <activity android:name=".Demo01_bindService.Main_Demo_bindService_01" />
        <activity android:name=".Demo02_startService.Main_Demo_startService_01" />
        <activity android:name=".Demo03_Foreground_Notification.Main_Demo_Foreground_Notification" />
        <activity android:name=".Demo05_Foreground_Media_Control.Main_Demo05" />
        <activity android:name=".Demo06.Main_Demo06" />

        <service android:name=".Demo01_bindService.LocalService" />
        <service android:name=".Demo02_startService.MyService" />

        <service android:name=".Demo03_Foreground_Notification.MyService" />
        <service android:name=".Demo05_Foreground_Media_Control.MyService" />
        <receiver android:name=".Demo05_Foreground_Media_Control.MyReceiver"/>
        <service android:name=".Demo06.MyService" />
        <receiver android:name=".Demo06.MyReceiver"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>